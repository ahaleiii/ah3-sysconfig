{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "console_title_template": "{{if .Root}}Admin: {{end}}{{.Shell}} ❯ {{.Folder}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} \ue0b1"
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "template": " Admin:"
        },
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "template": " {{ .Name }} \ue0b1"
        },
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "template": " {{ if .SSHSession }}\uf817 {{ end }}{{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#a00000",
          "template": " {{ .Path }} ",
          "properties": {
            "style": "agnoster_short",
            "hide_root_location": false,
            "max_depth": 4,
            "mapped_locations": {
              "C:/git": "\ue702 ",
              "C:/github": "\ue709 "
            }
          }
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#610099",
          "template": ".NET {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "files"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "foreground": "#ffffff",
          "background": "#00530b",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 <#ff4444>{{ .Working.String }}</>{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 <#4eff65>{{ .Staging.String }}</>{{ end }} ",
          "properties": {
            "fetch_upstream_icon": true,
            "git_icon": "\uf7a1 "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "powerline_symbol": "\ue0b2",
          "invert_powerline": true,
          "foreground": "#a7a400",
          "background": "#333333",
          "template": " <#ffff00>\ufbab</> {{ .FormattedMs }} |",
          "properties": {
            "style": "houston",
            "threshold": 1
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#a7a400",
          "background": "#333333",
          "template": " <#ffff00>\ue385</> {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "3:04:05.000 PM"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#ffffff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "template": "$❯",
          "properties": {
            "always_enabled": true
          }
        }
      ]
    }
  ],
  "secondary_prompt": {
    "foreground": "#ffffff",
    "template": " ❯ "
  }
}
